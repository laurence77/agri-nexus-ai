# AgriNexus AI Environment Configuration
# SECURITY: Never commit actual credentials to version control
# Copy this file to .env.local and fill in your actual values

# Environment
NODE_ENV=development
REACT_APP_APP_VERSION=1.0.0

# Demo Mode (ONLY for development)
DEMO_MODE=false
REACT_APP_DEMO_ADMIN_EMAIL=admin@example.com
REACT_APP_DEMO_ADMIN_PASSWORD=your-secure-admin-password
REACT_APP_DEMO_MANAGER_EMAIL=manager@example.com
REACT_APP_DEMO_MANAGER_PASSWORD=your-secure-manager-password
REACT_APP_DEMO_MFA_CODE=123456

# Logging Configuration
REACT_APP_LOG_LEVEL=info

# API Configuration
REACT_APP_API_BASE_URL=https://api.agrinexus.ai
REACT_APP_WS_URL=wss://ws.agrinexus.ai

# Third-party Services
REACT_APP_WEATHER_API_KEY=your-weather-api-key
REACT_APP_MAPS_API_KEY=your-maps-api-key

# Database (Backend)
DATABASE_URL=your-database-connection-string
SUPABASE_URL=your-supabase-url
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-key

# Authentication & Security
JWT_SECRET=your-jwt-secret-key
ENCRYPTION_KEY=your-encryption-key
SESSION_SECRET=your-session-secret

# Payment Services
PAYSTACK_PUBLIC_KEY=your-paystack-public-key
PAYSTACK_SECRET_KEY=your-paystack-secret-key
MPESA_CONSUMER_KEY=your-mpesa-consumer-key
MPESA_CONSUMER_SECRET=your-mpesa-consumer-secret
MPESA_BUSINESS_SHORT_CODE=your-mpesa-business-code
MPESA_PASSKEY=your-mpesa-passkey

# Email Service
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your-smtp-user
SMTP_PASS=your-smtp-password
FROM_EMAIL=noreply@agrinexus.ai

# Storage
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
S3_BUCKET_NAME=your-s3-bucket

# Analytics & Monitoring
GOOGLE_ANALYTICS_ID=your-ga-id
SENTRY_DSN=your-sentry-dsn
MIXPANEL_TOKEN=your-mixpanel-token

# Feature Flags
ENABLE_BLOCKCHAIN=false
ENABLE_AI_FEATURES=true
ENABLE_MOBILE_MONEY=true
ENABLE_OFFLINE_MODE=true

# Security Headers
CONTENT_SECURITY_POLICY="default-src 'self'; script-src 'self' 'unsafe-inline' https://www.googletagmanager.com; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; font-src 'self' https://fonts.gstatic.com; connect-src 'self' https://api.agrinexus.ai wss://ws.agrinexus.ai;"